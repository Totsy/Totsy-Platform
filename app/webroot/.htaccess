#<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /

    #RewriteCond %{HTTP_HOST} !^www\.totsystaging\.com [NC]
    #RewriteRule ^(.*)$ http://www.totsystaging.com/$1 [R=301,L]
    RewriteCond %{HTTP_HOST} !^www\.totsy\.com [NC]
    RewriteRule ^(.*)$ http://www.totsy.com/$1 [R=301,L]

    RewriteCond %{SERVER_PORT} !^443$
    RewriteRule ^checkout/(.*)$ https://%{SERVER_NAME}/checkout/$1 [NC,L]
    RewriteCond %{SERVER_PORT} !^443$
    RewriteRule ^cart/view$ https://%{SERVER_NAME}/checkout/view [NC,L]

    RewriteCond %{SERVER_PORT} !^80$
    RewriteRule ^sales/(.*)$ http://%{SERVER_NAME}%{REQUEST_URI} [NC,L]
    RewriteCond %{SERVER_PORT} !^80$      
    RewriteRule ^sales$ http://%{SERVER_NAME}%{REQUEST_URI} [NC,L]
    RewriteCond %{SERVER_PORT} !^80$
    RewriteRule ^sale/(.*)$ http://%{SERVER_NAME}%{REQUEST_URI} [NC,L]
    RewriteCond %{SERVER_PORT} !^80$
    RewriteRule ^account$ http://%{SERVER_NAME}%{REQUEST_URI} [NC,L]
    RewriteCond %{SERVER_PORT} !^80$
    RewriteRule ^account/(.*)$ http://%{SERVER_NAME}%{REQUEST_URI} [NC,L]
    RewriteCond %{SERVER_PORT} !^80$
    RewriteRule ^users/(.*)$ http://%{SERVER_NAME}%{REQUEST_URI} [NC,L]
    RewriteCond %{SERVER_PORT} !^80$
    RewriteRule ^pages/(.*)$ http://%{SERVER_NAME}%{REQUEST_URI} [NC,L]
    RewriteCond %{SERVER_PORT} !^80$
    RewriteRule ^tickets/(.*)$ http://%{SERVER_NAME}%{REQUEST_URI} [NC,L]

    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !favicon.ico$
    RewriteRule ^(.*)$ index.php?url=$1 [QSA,L]
#</IfModule>
